{
	"name": "Existing Dockerfile",
	"build": {
		"context": "..",
		"dockerfile": "../Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker"
			],
			"settings": {
				"dotfiles.repository": "https://github.com/Mkamono/dotfiles.git",
				"dotfiles.targetPath": "/workspaces/${localWorkspaceFolderBasename}/dotfiles"
			}
		}
	}
}
